package sangdepzai;

import java.util.Arrays;

public class test {
	public static void main(String[] args) {
		String[] club = { "volvo", "BMW", "Meccede" };
		club[0] = "xe cỏ";
		String[] a = new String[5];
		a[0] = "123";
		System.out.println("chạy từ mảng " + Arrays.toString(club));
		System.out.println("chạy từ mảng " + club[0]);
		System.out.println("chạy từ mảng " + a[0]);
	}
}
